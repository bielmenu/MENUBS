local Button = rev:CreateButton({
	Name = "Puxa Itens - Menos Lag",
	Callback = function()
		Rayfield:Notify({
			Title = "Aviso",
			Content = "Auto Puxar Itens Ativado, Pegara Comida!",
			Duration = 6.5,
			Image = 4483362458,
		})

		local function deletarNotifyGui()
			local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
			for _, gui in ipairs(playerGui:GetChildren()) do
				if gui.Name == "NotifyGui" and gui:IsA("ScreenGui") then
					gui:Destroy() -- Deleta a NotifyGui		
				end
			end
		end

		deletarNotifyGui()


		local itens = {"AR-15", "Agua", "Hamburguer", "Drink", "Pizza", "Escudo", "AK47", "Uzi", "PARAFAL", "Faca", "IA2", "G3", "Hi Power", "Natalina", "Tratamento", "C4"}

		local args = {
			[1] = "mudaInv",
			[2] = "2",
			[4] = "1"
		}

		local localPlayer = game.Players.LocalPlayer
		local playerName = localPlayer and localPlayer.Name or "Desconhecido"

		while true do
			if game.Players.LocalPlayer:FindFirstChild("RevistaPlayer") and game.Players.LocalPlayer:FindFirstChild("RevistaPlayer").Value ~= "" then
				for i, item in ipairs(itens) do
					if i <= 16 then  -- S√≥ tenta alocar at√© 16 slots
						args[3] = item  -- Atualiza o item para o valor da vez
						args[2] = tostring(i)
						args[4] = 1

						-- Usar task.spawn() para execu√ß√£o sem delay
						task.spawn(function()
							-- Envia a requisi√ß√£o para o servidor
							game:GetService("ReplicatedStorage"):WaitForChild("Modules"):WaitForChild("InvRemotes"):WaitForChild("InvRequest"):InvokeServer(unpack(args))
						end)
					end
				end
			end
			task.wait()  -- Espera um frame para evitar lag
		end
	end,
})


local Button = rev:CreateButton({
	Name = "Puxar Tudo - Menos Lag",
	Callback = function()

		local function deletarNotifyGui()
			local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
			for _, gui in ipairs(playerGui:GetChildren()) do
				if gui.Name == "NotifyGui" and gui:IsA("ScreenGui") then
					gui:Destroy()	
				end
			end
		end

		deletarNotifyGui()

		local ReplicatedStorage = game:GetService("ReplicatedStorage")
		local ToolsFolder = ReplicatedStorage:FindFirstChild("Tools")

		while true do
			if game.Players.LocalPlayer:FindFirstChild("RevistaPlayer") and game.Players.LocalPlayer:FindFirstChild("RevistaPlayer").Value ~= "" then


				if ToolsFolder then
					local Tools = ToolsFolder:GetChildren()

					for i, item in ipairs(Tools) do
						--if i <= 16 then
						local args = {}
						args[1] = "mudaInv"
						args[2] = tostring(i)
						args[3] = item.Name
						args[4] = 1

						task.spawn(function()
							local invRequest = ReplicatedStorage:WaitForChild("Modules"):WaitForChild("InvRemotes"):WaitForChild("InvRequest")
							invRequest:InvokeServer(unpack(args))
						end)
					end
					--end
				else
					warn("Pasta 'Tools' n√£o encontrada em ReplicatedStorage.")
				end
			end

			task.wait()

		end


	end,
})


