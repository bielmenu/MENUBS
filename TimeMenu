	Name = "CL Config",
	Range = {0, 100},
	Increment = 10,
	Suffix = "Vida",
	CurrentValue = 30,
	Flag = "CLConfig",
	Callback = function(Value)
		getgenv().HealthThreshold = Value
	end,
})

